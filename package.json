{
  "name": "react-experiment-repo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:scan": "npx react-scan@latest localhost:3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:check": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "lint:check": "biome lint ./src",
    "lint:fix": "biome lint ./src --fix",
    "type:performance": "tsc --generateTrace trace-output --incremental false",
    "type:check": "tsc --noEmit",
    "spell:check": "cspell . --gitignore --config cspell.config.json",
    "test": "vitest --coverage",
    "test:check": "vitest run",
    "check:all": "bun run format:check && bun run lint:check && bun run type:check && bun run spell:check && bun run test:check",
    "fix:all": "bun run format:fix && bun run lint:fix",
    "theme": "yamada-cli tokens ./src/theme/index.ts",
    "theme:watch": "yamada-cli tokens ./src/theme/index.ts --watch",
    "prepare": "bun theme",
    "depcheck": "bunx knip"
  },
  "dependencies": {
    "@conform-to/react": "^1.6.1",
    "@conform-to/valibot": "^1.6.1",
    "@next/third-parties": "^15.3.3",
    "@valibot/i18n": "^1.0.0",
    "@vercel/analytics": "^1.5.0",
    "@yamada-ui/lucide": "^1.10.5",
    "@yamada-ui/markdown": "^1.1.4",
    "@yamada-ui/react": "^1.7.7",
    "@yamada-ui/table": "^1.3.14",
    "jotai": "^2.12.5",
    "next": "15.3.4",
    "nodemailer": "^6.10.1",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "remeda": "^2.23.0",
    "server-only": "^0.0.1",
    "valibot": "^1.1.0",
    "valibot-env": "^0.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@types/node": "^22.15.31",
    "@types/nodemailer": "^6.4.17",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@yamada-ui/cli": "^1.2.4",
    "cspell": "^9.1.1",
    "typescript": "^5.8.3"
  }
}
